{
  "manifest_version": 2,
  "name": "SignTokent",
  "version": "1.0",
  "description": "Создание nft автографов",
  "icons": {
    "16": "logo16.png",
    "48": "logo48.png",
    "128": "logo128.png"
  },
  "permissions": [
    "tabs",
    "http://*/",
    "https://*/",
    "storage"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "register.html"
  }
}